{
	"instances_path": "./instances/",
    "usage_mode": {
        "instances_to_solve": ["inst02.dat"],
        "models_to_use": ["cp", "mip","sat","smt"]
    },
	"cp": {
		"solvers": ["gecode","chuffed","gecode-no-sym","chuffed-no-sym" ],
		"timeout": 300000,
		"processes": 16
	},
	"sat": {
	    "solvers": ["z3"],
		"timeout": 300000,
		"processes": 1
	},
    "mip": {
        "library": ["ortools", "mip", "pulp"],
        "mip_solvers": ["CBC"],
        "ortools_solvers": ["CBC", "SAT", "SCIP"],
        "pulp_solvers": ["CBC"],
        "timeout": 300,
        "processes": 1,
        "export_folder": "export/mip"
    },
	"smt": {
	    "solvers": ["z3"],
        "library": ["z3"],
        "timeout": 300,
        "processes": 1,
        "export_folder": "export/smt"
	}
}
