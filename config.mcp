{
	"instances_path":"./instances/",
    "usage_mode":{
                "instances_to_solve": "[inst01.dat]",
                "models_to_use":["mip", "smt"]
    },
	"cp":{
		"solvers":["gecode"],
		"timeout":300000,
		"processes":8
	},
	"sat":{
		"timeout":300000,
		"processes":1
	},
  "mip":{
    "library": ["pulp", "or-tools"],
    "timeout": 300000,
    "processes":8,
    "export_folder": "export/mip"
  },
	"smt":{
	  "library":["z3"],
	  "timeout":300000,
	  "processes":1,
	  "export_folder":"export/smt"

	}
}
